{
  "name": "@griseo.js/palette",
  "version": "1.0.0",
  "description": "colors and utilities - an artist palette to paint with ðŸŽ¨",
  "license": "MIT",
  "homepage": "https://elysium-everlasting.github.io/griseo.js/palette",
  "funding": "https://github.com/Elysium-Everlasting/griseo.js",
  "contributors": [
    {
      "name": "ap0nia",
      "email": "",
      "url": "https://github.com/ap0nia"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Elysium-Everlasting/griseo.js.git",
    "directory": "packages/palette"
  },
  "bugs": {
    "url": "https://github.com/Elysium-Everlasting/griseo.js/issues"
  },
  "keywords": [
    "color",
    "terminal",
    "console",
    "cli",
    "string",
    "ansi",
    "style",
    "formatting",
    "rgb",
    "text"
  ],
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    "./ansi": {
      "types": "./dist/ansi.d.ts",
      "import": "./dist/ansi.js",
      "require": "./dist/ansi.cjs",
      "default": "./dist/ansi.js"
    },
    "./sgr": {
      "types": "./dist/sgr.d.ts",
      "import": "./dist/sgr.js",
      "require": "./dist/sgr.cjs",
      "default": "./dist/sgr.js"
    },
    "./color-support": {
      "types": "./dist/color-support/index.d.ts",
      "import": "./dist/color-support/index.js",
      "require": "./dist/color-support/index.cjs",
      "default": "./dist/color-support/index.js"
    },
    "./color-support/*": {
      "types": "./dist/color-support/*.d.ts",
      "import": "./dist/color-support/*.js",
      "require": "./dist/color-support/*.cjs",
      "default": "./dist/color-support/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "ansi": [
        "./dist/ansi.d.ts"
      ],
      "sgr": [
        "./dist/sgr.d.ts"
      ],
      "color-support/*": [
        "./dist/color-support/*.d.ts"
      ]
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "tsup": "^7.1.0"
  },
  "packageManager": "pnpm@8.6.11",
  "engines": {
    "pnpm": "^8.0.0",
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
